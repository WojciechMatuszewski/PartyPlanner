{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": true,
        "labelFormat": "[local]"
      }
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    [
      "import",
      {
        "libraryName": "ant-design-pro",
        "libraryDirectory": "lib",
        "style": "css",
        "camel2DashComponentName": false
      },
      "ant-design-pro"
    ],

    ["emotion", { "inline": true }]
  ]
}
